    $(document).ready(function(){
        setInterval(changeIndexPic,5000);
        $("#memberpicdiv div img").live("click",function(){
            var uid = $(this).attr("attr");
            var url = "/user/view/id/1"
            url = url.replace("1",uid);
            // window.open(url);
			window.location.href=url;
        }).live("mouseenter",function(){
            var index = $("#memberpicdiv div img").index($(this));
            var uid = $(this).attr("attr");
            for(var i=0;i<allData.length;i++){
                if(allData[i]["uid"]==uid){
                    showIndexUserTip(i,index);
                    break;
                }
            }
        }).live("mouseleave",function(){
            $("#showInfo").mouseleave(function(){
                $("#showInfo").css("display","none");
            })
            
        });
    });
    function showIndexUserTip(i,index){
        $("#showInfo").css("display","");
        $("#showInfo span.nickname").html(allData[i]["nickname"]);
        $("#showInfo span.level").html(allData[i]["level"]);
        $("#showInfo span.area").html(allData[i]["area"]);
        $("#showInfo span.redboard").html(allData[i]["redboard"]);
        $("#showInfo span.blackboard").html(allData[i]["blackboard"]);
        $("#showInfo span.album").html(allData[i]["album"]);
        $("#showInfo span.visit").html(allData[i]["visit"]);
        /*定位*/
        var obj = $("#memberpicdiv div img").eq(index).offset();
        var w = $("#memberpicdiv div img").eq(index).width();
        var movey = 0;
        if(w>100){
            movey = -20;
        }
        if(w<50){
            movey = 20;
        }
        if(w<40){
            movey = 30;
        }
        $("#showInfo").css({
            left:obj.left-movey,
            top:obj.top-265
        });
    }
    
    function changeIndexPic(){
        var len = allData.length;
        var rand = Math.floor(Math.random()*(31-1)+1);
        var val = Math.floor(Math.random()*(len-2)+1);//可采用的数据
        $("#memberpicdiv div").eq(rand).fadeOut(1000,function(){
            $("#memberpicdiv div").eq(rand).html('<img attr="'+allData[val]["uid"]+'" src="'+allData[val]["photo"]+'"/>');
        }).fadeIn(1000);
    }
</script>	";i:1;N;}i:1;N;}